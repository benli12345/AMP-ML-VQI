ci.auc(test_5$DEAD_1yr, pred_5)

perf_1 <- prediction(pred_1, test_1$DEAD_1yr)
perf_1 <- performance(perf_1, "tpr", "fpr")

perf_2 <- prediction(pred_2, test_2$DEAD_1yr)
perf_2 <- performance(perf_2, "tpr", "fpr")

perf_3 <- prediction(pred_3, test_3$DEAD_1yr)
perf_3 <- performance(perf_3, "tpr", "fpr")

perf_4 <- prediction(pred_4, test_4$DEAD_1yr)
perf_4 <- performance(perf_4, "tpr", "fpr")

perf_5 <- prediction(pred_5, test_1$DEAD_1yr)
perf_5 <- performance(perf_5, "tpr", "fpr")

plot(perf_1)
plot(perf_2, add = TRUE, col = 'blue')
plot(perf_3, add = TRUE, col = 'orange')
plot(perf_4, add = TRUE, col = 'green')
plot(perf_5, add = TRUE, col = 'purple')
abline(a=0,b=1)

#Subgroup analysis based on urgency of intervention

#R version 4.3.1
#Load VQI AMP data (available with approval through SVS PSO https://www.vqi.org/data-analysis/)
library(readr)
data <- read_csv("amp_data.csv")

#Split data into train (70%) and test (30%) sets
library(caTools)
set.seed(123)
sample <- sample.split(data$DEAD_1yr, SplitRatio = 0.7)
train <- subset(data, sample == TRUE)
test <- subset(data, sample == FALSE)

#Apply Random Over-Sample Examples (ROSE) for class balance on training set
library(ROSE)
train <- ROSE(DEAD_1yr ~ ., data = train, N = 22828, seed = 123)$data

#Define predictors
library(dplyr)

predictors_train_preop <- train %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE)

predictors_test_preop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE)

predictors_train_intraop <- train %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE,ANESTHESIA, INCISION, DRESSING, EBL, TOTALPROCTIME)

predictors_test_intraop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE,ANESTHESIA, INCISION, DRESSING, EBL, TOTALPROCTIME)

predictors_train_postop <- train %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE,ANESTHESIA, INCISION, DRESSING, EBL, TOTALPROCTIME, POSTOP_SSI, POSTOP_MI, POSTOP_DYSRHYTHMIA, POSTOP_CHF, POSTOP_RESPIRATORY, CHGRENAL, TXFUSION, RTORBLEED, RTORINF, RTNORREV, DC_ASA, DC_P2Y, DC_STATIN, DC_BETABLOCKER, DC_ACE, DC_ANTICOAG, DC_AMBUL, TOTAL_LOS)

predictors_test_postop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE,ANESTHESIA, INCISION, DRESSING, EBL, TOTALPROCTIME, POSTOP_SSI, POSTOP_MI, POSTOP_DYSRHYTHMIA, POSTOP_CHF, POSTOP_RESPIRATORY, CHGRENAL, TXFUSION, RTORBLEED, RTORINF, RTNORREV, DC_ASA, DC_P2Y, DC_STATIN, DC_BETABLOCKER, DC_ACE, DC_ANTICOAG, DC_AMBUL, TOTAL_LOS)

#Train pre-op models
library(caret)
library(xgboost)
library(ranger)
library(naivebayes)
library(e1071)
library(nnet)
train_control <- trainControl(method = "cv", number = 10) #10-fold cross-validation

#Train XGBoost
XGBgrid <- expand.grid(max_depth = c(2,3,4,5,6,7,8,9), nrounds = (1:10)*50, eta = c(0.4,0.3,0.2,0.1,0.05,0.01,0.001), gamma = c(0,0.1,1,1.5,2), subsample = c(0.5,0.6,0.7,0.8,0.9,1), min_child_weight = c(1,3,5,7,10), colsample_bytree = c(0.5,0.6,0.7,0.8,0.9,1) #XGBoost grid search for hyperparameters
XGB <- train(x = as.matrix(predictors_train_preop), y = train$DEAD_1yr, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)

#Train random forest
RFgrid <- expand.grid(mtry = 2:4, splitrule = "gini", min.node.size = c(10,20))
RF <- train(x = predictors_train_preop, y = train$DEAD_1yr, data = train, method = "ranger", metric = "ROC", trControl = train_control, tuneGrid = RFgrid)

#Train naive bayes
NBgrid <- expand.grid(usekernel = c(TRUE, FALSE), fL = 0:5, adjust = seq(0,5,by=1))
NB <- train(x = predictors_train_preop, y = as.factor(train$DEAD_1yr), data = train, method = "naive_bayes", metric = "ROC", trControl = train_control, tuneGrid = NBgrid)

#Train support vector machine
SVMgrid <- expand.grid(C = c(0.001, 0.01, 0.1, 1, 10, 100, 10000))
SVM <- train(x = predictors_train_preop, y = train$DEAD_1yr, data = train, method = "svmRadial", metric = "ROC", trControl = train_control, tuneGrid = SVMgrid)

#Train artificial neural network
NNETgrid <- expand.grid(size = seq(1,10,by=1), decay = seq(0.1,0.5,by=0.1))
NNET <- train(x = predictors_train_preop, y = train$DEAD_1yr, data = train, method = "nnet", metric = "ROC", trControl = train_control, tuneGrid = NNETgrid)

#Train logistic regression
LRgrid <- expand.grid(C = c(0.001, 0.01, 0.1, 1, 10, 100, 10000))
LR <- train(x = predictors_train_preop, y = train$DEAD_1yr, data = train, method = "glm", metric = "ROC", trControl = train_control, tuneGrid = LRgrid, family = "binomial")

#Evaluate pre-op models on test set
library(pROC)

XGB_pred <- predict(XGB, as.matrix(predictors_test_preop))
auc(test$DEAD_1yr, XGB_pred)
ci.auc(test$DEAD_1yr, XGB_pred)
XGB_pred_0.5 <- ifelse(XGB_pred > 0.5,1,0)
confusionMatrix(as.factor(test$DEAD_1yr), as.factor(XGB_pred_0.5))

RF_pred <- predict(RF, predictors_test_preop)
auc(test$DEAD_1yr, RF_pred$predictions)
ci.auc(test$DEAD_1yr, RF_pred$predictions)
RF_pred_0.5 <- ifelse(RF_pred$predictions > 0.5,1,0)
confusionMatrix(as.factor(test$DEAD_1yr), as.factor(RF_pred_0.5))

NB_pred <- predict(NB, predictors_test_preop)
auc(test$DEAD_1yr, NB_pred)
ci.auc(test$DEAD_1yr, NB_pred)
NB_pred_0.5 <- ifelse(NB_pred > 0.5,1,0)
confusionMatrix(as.factor(test$DEAD_1yr), as.factor(NB_pred_0.5))

SVM_pred <- predict(SVM, predictors_test_preop)
auc(test$DEAD_1yr, SVM_pred)
ci.auc(test$DEAD_1yr, SVM_pred)
SVM_pred_0.5 <- ifelse(SVM_pred > 0.5,1,0)
confusionMatrix(as.factor(test$DEAD_1yr), as.factor(SVM_pred_0.5))

NNET_pred <- predict(NNET, predictors_test_preop)
auc(test$DEAD_1yr, NNET_pred)
ci.auc(test$DEAD_1yr, NNET_pred)
NNET_pred_0.5 <- ifelse(NNET_pred > 0.5,1,0)
confusionMatrix(as.factor(test$DEAD_1yr), as.factor(NNET_pred_0.5))

LR_pred <- predict(LR, predictors_test_preop)
auc(test$DEAD_1yr, LR_pred)
ci.auc(test$DEAD_1yr, LR_pred)
LR_pred_0.5 <- ifelse(LR_pred > 0.5,1,0)
confusionMatrix(as.factor(test$DEAD_1yr), as.factor(LR_pred_0.5))

#XGBoost selected as best performing model
#Train XGBoost on intra-operative and post-operative data
XGB_preop <- XGB
XGB_intraop <- train(x = as.matrix(predictors_train_intraop), y = train$DEAD_1yr, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)
XGB_postop <- train(x = as.matrix(predictors_train_postop), y = train$DEAD_1yr, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)

#Generate ROC curves
library(ROCR)
XGB_preop_pred <- predict(XGB_preop, as.matrix(predictors_test_preop))
XGB_preop_perf <- prediction(XGB_preop_pred, test$DEAD_1yr)
XGB_preop_perf <- performance(XGB_preop_perf, "tpr", "fpr")

XGB_intraop_pred <- predict(XGB_intraop, as.matrix(predictors_test_intraop))
XGB_intraop_perf <- prediction(XGB_intraop_pred, test$DEAD_1yr)
XGB_intraop_perf <- performance(XGB_intraop_perf, "tpr", "fpr")

XGB_postop_pred <- predict(XGB_postop, as.matrix(predictors_test_postop))
XGB_postop_perf <- prediction(XGB_postop_pred, test$DEAD_1yr)
XGB_postop_perf <- performance(XGB_postop_perf, "tpr", "fpr")

plot(XGB_preop_perf)
plot(XGB_intraop_perf, add = TRUE, col = 'blue')
plot(XGB_postop_perf, add = TRUE, col = 'orange')
abline(a=0,b=1)

#Generate calibration plots
library(gbm)
calibrate.plot(test$DEAD_1yr, XGB_preop_pred)
calibrate.plot(test$DEAD_1yr, XGB_intraop_pred)
calibrate.plot(test$DEAD_1yr, XGB_postop_pred)

#Calculate Brier Scores
library(DescTools)
BrierScore(XGB_preop_pred, test$DEAD_1yr)
BrierScore(XGB_intraop_pred, test$DEAD_1yr)
BrierScore(XGB_postop_pred, test$DEAD_1yr)

#Calculate variable importance scores for top 10 predictors
importance_matrix <- xgb.importance(names(predictors_test_postop), model = XGB_postop)
xgb.plot.importance(importance_matrix, top_n = 10)

#Subgroup analysis based on age
test_under65 <- test[test$AGE<65, ]
test_over65 <- test[test$AGE>=65, ]
predictors_test_under65 <- test_under65 %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE)
predictors_test_over65 <- test_over65 %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE)

pred_under65 <- predict(XGB_preop, as.matrix(predictors_test_under65))
auc(test_under65$DEAD_1yr, pred_under65)
ci.auc(test_under65$DEAD_1yr, pred_under65)

pred_over65 <- predict(XGB_preop, as.matrix(predictors_test_over65))
auc(test_over65$DEAD_1yr, pred_over65)
ci.auc(test_over65$DEAD_1yr, pred_over65)

perf_under65 <- prediction(pred_under65, test_under65$DEAD_1yr)
perf_under65 <- performance(perf_under65, "tpr", "fpr")

perf_over65 <- prediction(pred_over65, test_over65$DEAD_1yr)
perf_over65 <- performance(perf_over65, "tpr", "fpr")

plot(perf_under65)
plot(perf_over65, add = TRUE, col = 'blue')
abline(a=0,b=1)

#Subgroup analysis based on sex
test_male <- test[test$GENDER==1, ]
test_female <- test[test$GENDER==0, ]

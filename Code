predictors_test_female <- test_female %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, PREOP_DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, COPD, DIALYSIS, PRIOR_AMP_IPSI, PRIOR_AMP_CONTRA, PRIOR_INBYPASS_IPSI, PRIOR_INBYPASS_CONTRA, PRIOR_INPTASTENT_IPSI, PRIOR_INPTASTENT_CONTRA, PRIOR_LEGBYPASS_IPSI, PRIOR_LEGBYPASS_CONTRA, PRIOR_LEGPTASTENT_IPSI, PRIOR_LEGPTASTENT_CONTRA, PRIOR_CEACAS, PRIOR_CABG, PRIOR_PCI, ANEURREP, LIVINGSTATUS, PREOP_AMBUL, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, LEVEL, INDICATION, URGENCY, ASACLASS, SURGERYSIDE)

pred_male <- predict(XGB_preop, as.matrix(predictors_test_male))
